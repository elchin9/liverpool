
@{
    @model StaticData
  Layout = "_AdminLayout";
    ViewData["Title"] = "Details";
}

<div class="page-container">
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <section class="wrapper" style="padding-bottom:50px;">
                    <div class="row">
                        
                        <div class="col-12 mt-4">
                            <h3>@Model.Email</h3>
                            <p class="mt-5">@Model.FacebookLink</p>
                            <p>@Model.TwitterLink</p>
                            <p>@Model.YoutubeLink</p>
                            <p>@Model.InstagramLink</p>
                            <p>@Model.Phone</p>
                        </div>
                    </div>
                    <div class="nd-content">
                        
                    </div>

                    <div class="row">
                        <div class="col-12 text-center">
                            <a class="btn btn-primary mt-5" asp-action="Edit" asp-route-id="@Model.Id">Dəyişdir</a>
                            <a class="btn btn-danger mt-5" asp-action="Details">Əsas Səhifəyə qayıt</a>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>

